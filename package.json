  "description": "three.js plugin for loading voxel data",
  "main": "lib/voxel-loader.js",
  "module": "lib/voxel-loader.module.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "test": "jest --passWithNoTests",
